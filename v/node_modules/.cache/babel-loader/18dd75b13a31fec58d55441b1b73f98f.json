AA,QAAA,EAAA,EAjBA;AAkBA,MAAA,WAAA,EAAA;AAlBA,KAAA;AAoBA,GA3BA;AA4BA,EAAA,OAAA,EAAA;AACA;AACA,IAAA,MAFA,kBAEA,KAFA,EAEA;AACA,WAAA,SAAA,CAAA,KAAA,GAAA,KAAA;AACA,WAAA,WAAA;AACA,KALA;AAMA;AACA,IAAA,KAPA,iBAOA,IAPA,EAOA;AACA,WAAA,SAAA,CAAA,KAAA,GAAA,IAAA,CAAA,KAAA;AACA,WAAA,SAAA,CAAA,IAAA,GAAA,IAAA,CAAA,IAAA;AACA,WAAA,WAAA;AACA,KAXA;AAYA;AACA,IAAA,eAbA,2BAaA,CAbA,EAaA,CAbA,EAaA;AACA,UAAA,CAAA,IAAA,KAAA,EAAA;AACA,aAAA,UAAA,GAAA,CAAA,CAAA,UAAA;AACA,aAAA,OAAA,GAAA;AAAA,UAAA,MAAA,EAAA;AAAA,SAAA;;AACA,YAAA,CAAA,CAAA,IAAA,IAAA,SAAA,EAAA;AACA,eAAA,WAAA;AACA;AACA;;AACA,UAAA,CAAA,IAAA,MAAA,EAAA;AACA,aAAA,WAAA,GAAA,CAAA,CAAA,UAAA;;AACA,YAAA,CAAA,CAAA,IAAA,IAAA,SAAA,EAAA;AACA,eAAA,WAAA;AACA;AACA;AACA,KA3BA;AA4BA;AACA,IAAA,IA7BA,gBA6BA,IA7BA,EA6BA;AACA,WAAA,WAAA,GAAA,IAAA;AACA,WAAA,QAAA,GAAA,IAAA;AACA,KAhCA;AAiCA;AACA,IAAA,UAlCA,sBAkCA,IAlCA,EAkCA;AAAA;;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,KAAA,CAAA,MAAA,CAAA,GAAA,CAAA,uBAAA,EAAA;AACA,kBAAA,MAAA,EAAA;AACA,oBAAA,EAAA,EAAA,IAAA,CAAA,EADA;AAEA,oBAAA,MAAA,EAAA,IAAA,CAAA;AAFA;AADA,iBAAA,CADA;;AAAA;AAAA;AACA,gBAAA,GADA,yBACA,IADA;;AAAA,sBAOA,GAAA,CAAA,IAAA,KAAA,CAPA;AAAA;AAAA;AAAA;;AAQA,gBAAA,IAAA,CAAA,MAAA,GAAA,CAAA,IAAA,CAAA,MAAA;AARA,iDASA,KAAA,CAAA,QAAA,CAAA,KAAA,CAAA,QAAA,CATA;;AAAA;AAWA,gBAAA,KAAA,CAAA,QAAA,CAAA,OAAA,CAAA,QAAA;;AAXA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAYA,KA9CA;AA+CA;AACA,IAAA,UAhDA,sBAgDA,EAhDA,EAgDA;AAAA;;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA,qBACA,OAAA,CAAA,SAAA,CADA;AAAA;AAAA;AAAA;;AAAA;AAAA,uBAEA,MAAA,CAAA,MAAA,CAAA,GAAA,CAAA,kBAAA,EAAA;AAAA,kBAAA,MAAA,EAAA;AAAA,oBAAA,EAAA,EAAA;AAAA;AAAA,iBAAA,CAFA;;AAAA;AAAA;AAEA,gBAAA,GAFA,yBAEA,IAFA;;AAAA,sBAGA,GAAA,CAAA,IAAA,KAAA,CAHA;AAAA;AAAA;AAAA;;AAAA,kDAGA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,GAAA,CAAA,GAAA,CAHA;;AAAA;AAIA,gBAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA;;AACA,gBAAA,MAAA,CAAA,WAAA;;AALA;AAAA;;AAAA;AAAA,kDAOA,KAPA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AASA,KAzDA;AA2DA,IAAA,WA3DA,yBA2DA;AAAA;;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,MAAA,CAAA,MAAA,CAAA,GAAA,CAAA,iBAAA,EAAA;AAAA,kBAAA,MAAA,EAAA,MAAA,CAAA;AAAA,iBAAA,CADA;;AAAA;AAAA;AACA,gBAAA,GADA,yBACA,IADA;AAEA,gBAAA,MAAA,CAAA,QAAA,GAAA,GAAA,CAAA,IAAA;AACA,gBAAA,MAAA,CAAA,KAAA,GAAA,GAAA,CAAA,KAAA;;AAHA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAIA;AA/DA;AA5BA,CAAA","sourcesContent":["<template>\r\n    <div>\r\n        <!--导航区-->\r\n        <Crumbs :cru=\"cru\"></Crumbs>\r\n        <!--卡片视图区-->\r\n        <el-card class=\"box-card\">\r\n            <!--添加查询-->\r\n            <el-row :gutter=\"20\">\r\n                <!--查询-->\r\n                <Search :queryInfo=\"queryInfo\" :search=\"search\"></Search>\r\n                <el-col :span=\"4\">\r\n                    <el-button type=\"primary\" @click=\"add_dialog=true\">添加用户</el-button>\r\n                </el-col>\r\n            </el-row>\r\n\r\n            <!--用户列表区-->\r\n            <el-table :data=\"userInfo\" border stripe style=\"margin-top: 10px;\">\r\n                <el-table-column type=\"index\" label=\"#\" width=\"50\"></el-table-column>\r\n                <el-table-column prop=\"username\"  label=\"用户名\" width=\"150\"></el-table-column>\r\n                <el-table-column prop=\"groupid\"  label=\"角色\" width=\"150\"></el-table-column>\r\n                <el-table-column label=\"状态\" width=\"150\" >\r\n                    <template slot-scope=\"scope\">\r\n                        <el-switch\r\n                                v-model=\"scope.row.status\"\r\n                                :active-value=\"1\" :inactive-value=\"0\"\r\n                                @change=\"editStatus(scope.row)\"\r\n                                active-color=\"#13ce66\"\r\n                                inactive-color=\"#ff4949\">\r\n                        </el-switch>\r\n                    </template>\r\n                </el-table-column>\r\n                <el-table-column prop=\"create_time\" label=\"创建时间\"  ></el-table-column>\r\n                <el-table-column label=\"操作\" width=\"150\" >\r\n                    <template slot-scope=\"scope\">\r\n                        <el-button type=\"primary\"  icon=\"el-icon-edit\" @click=\"edit(scope.row)\" circle></el-button>\r\n                        <el-button type=\"danger\" icon=\"el-icon-delete\" @click=\"userdelete(scope.row.id)\" circle></el-button>\r\n                    </template>\r\n                </el-table-column>\r\n            </el-table>\r\n\r\n            <!--分页效果组件-->\r\n            <Page :query=\"queryInfo\" :total=\"total\" :pages=\"pages\" :pageSizes=\"pageSizes\"></Page>\r\n\r\n\r\n            <!--添加弹窗-->\r\n            <Addform v-if=\"add_dialog\" :add_dialog=\"add_dialog\" :form=\"addform\" @closeDialog=\"closeDialogFunc($event, 'add')\"></Addform>\r\n            <!--修改弹窗-->\r\n            <Editform v-if=\"edit_dialog\" :edit_dialog=\"edit_dialog\" :form=\"editform\" @closeDialog=\"closeDialogFunc($event, 'edit')\"></Editform>\r\n\r\n\r\n        </el-card>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n    import Page from '../common/Page'\r\n    import Crumbs from '../common/Crumbs'\r\n    import Search from '../common/Search'\r\n    import Addform from '../../views/admin/Addform'\r\n    import Editform from '../../views/admin/Editform'\r\n    export default {\r\n        name: \"Users\",\r\n        components:{Page,Crumbs,Search,Addform,Editform},\r\n        created(){\r\n            this.getUserList()\r\n        },\r\n        data(){\r\n            return {\r\n                // 列表数据\r\n                userInfo:[],\r\n                // 分页搜索数据\r\n                total:1,\r\n                pageSizes:[1,5,10,20],\r\n                queryInfo:{\r\n                    query:'',\r\n                    page:1,\r\n                    limit:5\r\n                },\r\n                // 面包屑导航数据\r\n                cru:['用户管理','用户列表'],\r\n                // 添加\r\n                addform:{status:1},\r\n                add_dialog:false,\r\n                // 修改\r\n                editform:{},\r\n                edit_dialog:false,\r\n            }\r\n        },\r\n        methods:{\r\n            // 模糊查询\r\n            search(query){\r\n                this.queryInfo.query=query\r\n                this.getUserList()\r\n            },\r\n            //分页\r\n            pages(info){\r\n                this.queryInfo.limit=info.limit\r\n                this.queryInfo.page=info.page\r\n                this.getUserList()\r\n            },\r\n            //回调\r\n            closeDialogFunc(e, f) {\r\n                if (f == 'add') {\r\n                    this.add_dialog = e.openDialog;\r\n                    this.addform={status:1}\r\n                    if (e.type == 'success') {\r\n                        this.getUserList();\r\n                    }\r\n                }\r\n                if (f == 'edit') {\r\n                    this.edit_dialog = e.openDialog;\r\n                    if (e.type == 'success') {\r\n                        this.getUserList();\r\n                    }\r\n                }\r\n            },\r\n            // 修改\r\n            edit(info){\r\n                this.edit_dialog=true\r\n                this.editform=info\r\n            },\r\n            //修改状态\r\n            async editStatus(info){\r\n                const {data: res} = await this.$axios.get(\"/api/admin/editStatus\", {\r\n                    params: {\r\n                        id: info.id,\r\n                        status: info.status\r\n                    }\r\n                });\r\n                if(res.code!==0){\r\n                    info.status=!info.status\r\n                    return this.$message.error('更新状态失敗')\r\n                }\r\n                this.$message.success('更新状态成功')\r\n            },\r\n            //删除\r\n            async userdelete(id){\r\n                if(confirm('确定要删除吗？')){\r\n                    const {data:res}=await this.$axios.get('api/admin/delete',{params:{id:id}})\r\n                        if(res.code!==0) return this.$message.error(res.msg)\r\n                        this.$message.success('删除成功')\r\n                        this.getUserList()\r\n                }else{\r\n                    return false\r\n                }\r\n            },\r\n\r\n            async getUserList(){\r\n              const {data:res}=await this.$axios.get('api/admin/index',{params:this.queryInfo})\r\n                this.userInfo=res.data\r\n                this.total=res.total\r\n            },\r\n\r\n        },\r\n    }\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"],"sourceRoot":"src/components/admin"}]}