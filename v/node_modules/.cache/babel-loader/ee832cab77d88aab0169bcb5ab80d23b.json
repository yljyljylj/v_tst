;AAAA;AAAA;AAAA;AAAA;AAAA;AAOA,KARA;AASA,IAAA,cATA,0BASA,CATA,EASA;AACA;AACA,UAAA,CAAA,EAAA;AACA,aAAA,KAAA,CAAA,aAAA,EAAA;AACA,UAAA,IAAA,EAAA,SADA;AAEA,UAAA,UAAA,EAAA;AAFA,SAAA;AAIA,OALA,MAKA;AACA,aAAA,KAAA,CAAA,aAAA,EAAA;AACA,UAAA,IAAA,EAAA,OADA;AAEA,UAAA,UAAA,EAAA;AAFA,SAAA;AAIA;AACA;AAtBA;AAXA,CAAA","sourcesContent":["<template>\r\n    <el-dialog title=\"新增角色\" :visible.sync=\"dialogVisible\">\r\n        <el-form :model=\"form\">\r\n            <el-form-item label=\"角色名称\" :label-width=\"formLabelWidth\"\r\n                          prop=\"name\"\r\n                          :rules=\"[{ required: true, message: '角色名称不能为空'},]\">\r\n                <el-input v-model.trim=\"form.name\" autocomplete=\"off\"></el-input>\r\n            </el-form-item>\r\n            <el-form-item label=\"角色状态\" :label-width=\"formLabelWidth\">\r\n                <el-radio v-model=\"form.status\" :label=\"1\">开启</el-radio>\r\n                <el-radio v-model=\"form.status\" :label=\"0\">禁用</el-radio>\r\n            </el-form-item>\r\n            <el-form-item label=\"角色描述\" :label-width=\"formLabelWidth\">\r\n                <el-input type=\"textarea\" v-model=\"form.title\"></el-input>\r\n            </el-form-item>\r\n        </el-form>\r\n        <div slot=\"footer\" class=\"dialog-footer\">\r\n            <el-button @click=\"resetAddDialog(false)\">取 消</el-button>\r\n            <el-button type=\"primary\" @click=\"addRules\">确 定</el-button>\r\n        </div>\r\n    </el-dialog>\r\n</template>\r\n\r\n<script>\r\n    export default {\r\n        name: \"Addform\",\r\n        props:['add_dialog','form'],\r\n        data(){\r\n            return{\r\n                /*左边长度*/\r\n                formLabelWidth: '180px',\r\n                /*是否显示*/\r\n                dialogVisible: this.add_dialog,\r\n            }\r\n        },\r\n        methods:{\r\n            async addRules() {\r\n                if(this.form.name==='' || this.form.name===null || this.form.name===undefined) return this.$message.error('角色名称不能为空')\r\n                const {data:res}=await this.$axios.post('api/auth/add',this.form)\r\n                if(res.code!==0) return this.$message.error('添加失败')\r\n                this.$message.success('添加成功')\r\n                this.resetAddDialog(true)\r\n\r\n            },\r\n            resetAddDialog(e){\r\n                // console.log(e)\r\n                if (e) {\r\n                    this.$emit('closeDialog', {\r\n                        type: 'success',\r\n                        openDialog: false\r\n                    })\r\n                } else {\r\n                    this.$emit('closeDialog', {\r\n                        type: 'error',\r\n                        openDialog: false\r\n                    })\r\n                }\r\n            }\r\n        }\r\n    }\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"],"sourceRoot":"src/components/auth"}]}