,GAAA,EAAA;AACA,4BAAA,MAAA,CAAA,qBAAA,GAAA,KAAA;;AACA,4BAAA,MAAA,CAAA,WAAA;;AAPA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,mBAAA;;AAAA;AAAA;AAAA;AAAA;;AADA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAWA,KAhDA;AAiDA,IAAA,SAjDA,uBAiDA;AACA,WAAA,KAAA,CAAA,WAAA,CAAA,WAAA;AACA,WAAA,QAAA,GAAA,EAAA;AACA,KApDA;AAqDA,IAAA,UArDA,sBAqDA,IArDA,EAqDA;AAAA;;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,MAAA,CAAA,MAAA,CAAA,GAAA,CAAA,sBAAA,EAAA;AACA,kBAAA,MAAA,EAAA;AACA,oBAAA,EAAA,EAAA,IAAA,CAAA,EADA;AAEA,oBAAA,MAAA,EAAA,IAAA,CAAA;AAFA;AADA,iBAAA,CADA;;AAAA;AAAA;AACA,gBAAA,GADA,yBACA,IADA;;AAAA,sBAOA,GAAA,CAAA,IAAA,KAAA,CAPA;AAAA;AAAA;AAAA;;AAQA,gBAAA,IAAA,CAAA,MAAA,GAAA,CAAA,IAAA,CAAA,MAAA;AARA,kDASA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,QAAA,CATA;;AAAA;AAAA,kDAWA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,QAAA,CAXA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAaA,KAlEA;AAqEA;AACA,IAAA,UAtEA,sBAsEA,EAtEA,EAsEA;AAAA;;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA,qBACA,OAAA,CAAA,SAAA,CADA;AAAA;AAAA;AAAA;;AAAA;AAAA,uBAEA,MAAA,CAAA,MAAA,CAAA,GAAA,CAAA,iBAAA,EAAA;AAAA,kBAAA,MAAA,EAAA;AAAA,oBAAA,EAAA,EAAA;AAAA;AAAA,iBAAA,CAFA;;AAAA;AAAA;AAEA,gBAAA,GAFA,yBAEA,IAFA;;AAAA,sBAGA,GAAA,CAAA,IAAA,KAAA,CAHA;AAAA;AAAA;AAAA;;AAAA,kDAGA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,GAAA,CAAA,GAAA,CAHA;;AAAA;AAIA,gBAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA;;AACA,gBAAA,MAAA,CAAA,WAAA;;AALA;AAAA;;AAAA;AAAA,kDAOA,KAPA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AASA,KA/EA;AAiFA,IAAA,WAjFA,yBAiFA;AAAA;;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,MAAA,CAAA,MAAA,CAAA,GAAA,CAAA,gBAAA,EAAA;AAAA,kBAAA,MAAA,EAAA,MAAA,CAAA;AAAA,iBAAA,CADA;;AAAA;AAAA;AACA,gBAAA,GADA,yBACA,IADA;AAEA,gBAAA,MAAA,CAAA,QAAA,GAAA,GAAA,CAAA,IAAA;AACA,gBAAA,MAAA,CAAA,KAAA,GAAA,GAAA,CAAA,KAAA;;AAHA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAIA;AArFA;AA9CA,CAAA","sourcesContent":["<template>\r\n    <div>\r\n        <!--导航区-->\r\n        <Crumbs :cru=\"cru\"></Crumbs>\r\n        <!--卡片视图区-->\r\n        <el-card class=\"box-card\">\r\n            <!--添加查询-->\r\n            <el-row :gutter=\"20\">\r\n                <!--查询-->\r\n                <Search :queryInfo=\"queryInfo\" :search=\"search\"></Search>\r\n                <el-col :span=\"4\">\r\n                    <el-button type=\"primary\" @click=\"userSave\">添加用户</el-button>\r\n                </el-col>\r\n            </el-row>\r\n\r\n            <!--用户列表区-->\r\n            <el-table :data=\"userInfo\" border stripe style=\"margin-top: 10px;\">\r\n                <el-table-column prop=\"id\"  label=\"#\"  width=\"50\"></el-table-column>\r\n                <el-table-column prop=\"username\"  label=\"用户名\" width=\"150\"></el-table-column>\r\n                <el-table-column prop=\"groupid\"  label=\"角色\" width=\"150\"></el-table-column>\r\n                <el-table-column label=\"状态\" width=\"150\" >\r\n                    <template slot-scope=\"scope\">\r\n                        <el-switch\r\n                                v-model=\"scope.row.status\"\r\n                                :active-value=\"1\" :inactive-value=\"0\"\r\n                                @change=\"editStatus(scope.row)\"\r\n                                active-color=\"#13ce66\"\r\n                                inactive-color=\"#ff4949\">\r\n                        </el-switch>\r\n                        <!--<el-button type=\"success\" v-if=\"scope.row.status===1\" size=\"mini\" disabled>正常</el-button>-->\r\n                        <!--<el-button type=\"danger\" v-if=\"scope.row.status!==1\" size=\"mini\" disabled>禁用</el-button>-->\r\n                    </template>\r\n                </el-table-column>\r\n                <el-table-column prop=\"create_time\" label=\"创建时间\"  ></el-table-column>\r\n                <el-table-column label=\"操作\" width=\"150\" >\r\n                    <template slot-scope=\"scope\">\r\n                        <el-button type=\"primary\"  icon=\"el-icon-edit\" @click=\"edit(scope.row)\" circle></el-button>\r\n                        <el-button type=\"danger\" icon=\"el-icon-delete\" @click=\"userdelete(scope.row.id)\" circle></el-button>\r\n                        <!--<el-tooltip class=\"item\" effect=\"dark\" content=\"设置角色\" placement=\"top\" :enterable=\"false\">-->\r\n                            <!--<el-button type=\"info\" icon=\"el-icon-setting\"  circle></el-button>-->\r\n                        <!--</el-tooltip>-->\r\n                    </template>\r\n                </el-table-column>\r\n            </el-table>\r\n\r\n            <!--分页效果组件-->\r\n            <Page :query=\"queryInfo\" :total=\"total\" :pages=\"pages\" :pageSizes=\"pageSizes\"></Page>\r\n\r\n\r\n            <!--添加弹窗-->\r\n            <el-dialog title=\"添加用户\" :visible.sync=\"savedialogFormVisible\" @close=\"addClose\">\r\n            <el-form :model=\"userform\" ref=\"saveForm\" :rules=\"rules\">\r\n                <el-form-item label=\"用户名\" :label-width=\"formLabelWidth\" prop=\"username\">\r\n                    <el-input v-model=\"userform.username\" autocomplete=\"off\"></el-input>\r\n                </el-form-item>\r\n                <el-form-item label=\"密 码\" :label-width=\"formLabelWidth\" prop=\"password\">\r\n                    <el-input type=\"password\" v-model=\"userform.password\" autocomplete=\"off\" ></el-input>\r\n                </el-form-item>\r\n\r\n                <el-form-item label=\"角 色\" :label-width=\"formLabelWidth\">\r\n                    <el-select v-model=\"userform.groupid\" placeholder=\"请选择用户角色\">\r\n                        <el-option label=\"炒鸡管理员\" :value=\"1\"></el-option>\r\n                        <el-option label=\"用户管理员\" :value=\"2\"></el-option>\r\n                    </el-select>\r\n                </el-form-item>\r\n\r\n                <el-form-item label=\"用户状态\" :label-width=\"formLabelWidth\">\r\n                    <el-radio v-model=\"userform.status\" :label=\"1\">启用</el-radio>\r\n                    <el-radio v-model=\"userform.status\" :label=\"0\">禁用</el-radio>\r\n                </el-form-item>\r\n            </el-form>\r\n            <div slot=\"footer\" class=\"dialog-footer\">\r\n                <el-button @click=\"savedialogFormVisible = false\">取 消</el-button>\r\n                <el-button type=\"primary\" @click=\"save\">确 定</el-button>\r\n            </div>\r\n        </el-dialog>\r\n\r\n            <!--修改弹窗-->\r\n            <el-dialog title=\"修改信息\" :visible.sync=\"editdialogFormVisible\" @close=\"editClose\">\r\n                <el-form :model=\"editform\" ref=\"editFormRef\" :rules=\"rules\">\r\n                    <el-form-item label=\"用户名\" :label-width=\"formLabelWidth\" prop=\"username\">\r\n                        <el-input v-model=\"editform.username\" autocomplete=\"off\"></el-input>\r\n                    </el-form-item>\r\n                    <el-form-item label=\"密 码\" :label-width=\"formLabelWidth\" prop=\"password\">\r\n                        <el-input type=\"password\" v-model=\"editform.password\" autocomplete=\"off\" ></el-input>\r\n                    </el-form-item>\r\n\r\n                    <el-form-item label=\"角 色\" :label-width=\"formLabelWidth\">\r\n                        <el-select v-model=\"editform.groupid\" placeholder=\"请选择用户角色\">\r\n                            <el-option label=\"炒鸡管理员\" :value=\"1\"></el-option>\r\n                            <el-option label=\"用户管理员\" :value=\"2\"></el-option>\r\n                        </el-select>\r\n                    </el-form-item>\r\n\r\n                    <el-form-item label=\"用户状态\" :label-width=\"formLabelWidth\">\r\n                        <el-radio v-model=\"editform.status\" :label=\"1\">启用</el-radio>\r\n                        <el-radio v-model=\"editform.status\" :label=\"0\">禁用</el-radio>\r\n                    </el-form-item>\r\n                </el-form>\r\n                <div slot=\"footer\" class=\"dialog-footer\">\r\n                    <el-button @click=\"editdialogFormVisible = false\">取 消</el-button>\r\n                    <el-button type=\"primary\" @click=\"edits\">确 定</el-button>\r\n                </div>\r\n            </el-dialog>\r\n\r\n        </el-card>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n    import Page from '../common/Page'\r\n    import Crumbs from '../common/Crumbs'\r\n    import Search from '../common/Search'\r\n    export default {\r\n        name: \"Users\",\r\n        components:{Page,Crumbs,Search},\r\n        created(){\r\n            this.getUserList()\r\n        },\r\n        data(){\r\n            return {\r\n                // 列表数据\r\n                userInfo:[],\r\n                // 分页搜索数据\r\n                total:1,\r\n                pageSizes:[1,5,10,20],\r\n                queryInfo:{\r\n                    query:'',\r\n                    page:1,\r\n                    limit:5\r\n                },\r\n                // 面包屑导航数据\r\n                cru:['用户管理','用户列表'],\r\n\r\n                // 添加\r\n                userform:{\r\n                    groupid:1,\r\n                    status:1\r\n                },\r\n                savedialogFormVisible:false,\r\n                formLabelWidth:'100px',\r\n\r\n                // 修改\r\n                editform:{},\r\n                editdialogFormVisible:false,\r\n\r\n                // 验证规则\r\n                rules: {\r\n                    username: [\r\n                        {required: true, message: '请输入账号', trigger: 'blur'},\r\n                    ],\r\n                    password: [\r\n                        {required: true, message: '请输入密码', trigger: 'change'}\r\n                    ],\r\n\r\n                }\r\n\r\n            }\r\n        },\r\n        methods:{\r\n            // 模糊查询\r\n            search(query){\r\n                this.queryInfo.query=query\r\n                this.getUserList()\r\n            },\r\n            //分页\r\n            pages(info){\r\n                this.queryInfo.limit=info.limit\r\n                this.queryInfo.page=info.page\r\n                this.getUserList()\r\n            },\r\n\r\n            // 添加\r\n            userSave(){\r\n                this.savedialogFormVisible=true\r\n            },\r\n            save(){\r\n                this.$refs.saveForm.validate(async vild=>{\r\n                    if(!vild) return false\r\n                    const {data:res}=await this.$axios.post('api/user/save',this.userform)\r\n                    if(res.code!==0) return this.$message.error(res.msg)\r\n                    this.$message.success('添加成功')\r\n                    this.userform={groupid:1,status:1}\r\n                    this.getUserList()\r\n                    this.savedialogFormVisible=false\r\n                })\r\n            },\r\n            addClose(){\r\n                this.$refs.saveForm.resetFields()\r\n            },\r\n\r\n            // 修改\r\n            edit(info){\r\n                this.editdialogFormVisible=true\r\n                this.editform=info\r\n            },\r\n            async edits(){\r\n                this.$refs.editFormRef.validate(async vild=>{\r\n                    if(!vild) return false\r\n                    const {data:res}=await this.$axios.post('api/user/edit',this.editform)\r\n                    if(res.code!==0) return this.$message.error('修改失败')\r\n                    this.$message.success('修改成功')\r\n                    this.editform={}\r\n                    this.editdialogFormVisible=false\r\n                    this.getUserList()\r\n                })\r\n\r\n            },\r\n            editClose(){\r\n                this.$refs.editFormRef.resetFields()\r\n                this.editform={}\r\n            },\r\n            async editStatus(info){\r\n                const {data: res} = await this.$axios.get(\"/api/user/editStatus\", {\r\n                    params: {\r\n                        id: info.id,\r\n                        status: info.status\r\n                    }\r\n                });\r\n                if(res.code!==0){\r\n                    info.status=!info.status\r\n                    return this.$message.error('更新状态失敗')\r\n                }\r\n                return this.$message.success('更新状态成功')\r\n                this.getUserList()\r\n            },\r\n\r\n\r\n            //删除\r\n            async userdelete(id){\r\n                if(confirm('确定要删除吗？')){\r\n                    const {data:res}=await this.$axios.get('api/user/delete',{params:{id:id}})\r\n                        if(res.code!==0) return this.$message.error(res.msg)\r\n                        this.$message.success('删除成功')\r\n                        this.getUserList()\r\n                }else{\r\n                    return false\r\n                }\r\n            },\r\n\r\n            async getUserList(){\r\n              const {data:res}=await this.$axios.get('api/user/index',{params:this.queryInfo})\r\n                this.userInfo=res.data\r\n                this.total=res.total\r\n            },\r\n\r\n\r\n        },\r\n    }\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"],"sourceRoot":"src/components/user"}]}