;\r\n                    if (e.type == 'success') {\r\n                        this.getUserList();\r\n                    }\r\n                }\r\n            },\r\n            async getAuthList(){\r\n                const {data:res}=await this.$axios.get('api/auth/index',{params:this.queryInfo})\r\n                if(res.code===0) return this.authList=res.data\r\n            },\r\n        }\r\n    }\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"]}]}